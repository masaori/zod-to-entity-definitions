{
  "name": "@masaori/zod-to-entity-definitions",
  "version": "1.1.5",
  "description": "A library to define data models using Zod with extended metadata and convert them into framework-agnostic Entity Definitions",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "fix": "biome check --write .",
    "format": "biome format --write .",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "npm run check-types && npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "zod",
    "entity",
    "orm",
    "schema",
    "typescript",
    "entity-definition",
    "er-model"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/masaori/zod-to-entity-definitions.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "peerDependencies": {
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.0",
    "@types/node": "^22.10.1",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "semantic-release": "^24.0.0",
    "tsup": "^8.3.5",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "zod": "4.1.13"
  }
}
